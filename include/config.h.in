#define CPU_VENDOR_OS "${CPU_VENDOR_OS}"

/* 如果是在Linux系統上，則定義之 */
#cmakedefine _GNU_SOURCE

/* 如果相應的頭文件存在則由cmake定義 */
#cmakedefine HAVE_STRINGS_H 1  /* strings.h */
#cmakedefine HAVE_STRING_H 1   /* string.h */
#cmakedefine HAVE_NETINET_IN_H 1  /* netinet/in.h */
#cmakedefine HAVE_ARPA_INET_H 1   /* arpa/inet.h */
#cmakedefine HAVE_NETINET_TCP_H 1 /* netinet/tcp.h */
#cmakedefine HAVE_NETINET_UDP_H 1 /* netinet/udp.h */
#cmakedefine HAVE_NETINET_SCTP_H 1  /* netinet/sctp.h */

/* 如果函數存在則由cmake定義*/
#cmakedefine HAVE_BZERO 1  /* bzero */
#cmakedefine HAVE_SNPRINTF 1 /* snprintf */
#cmakedefine HAVE_VSNPRINTF 1  /* svnprintf */
#cmakedefine HAVE_INET_PTON 1  /* inet_pton */
#cmakedefine HAVE_INET_NTOP 1  /* inet_ntop */

/* 檢查符號是否定義 */
#cmakedefine HAVE_INET_ADDRSTRLEN  /* INET_ADDRSTRLEN in <netinet/in.h> */
#cmakedefine HAVE_INET6_ADDRSTRLEN /* INET6_ADDRSTRLEN in <netinet/in.h> */


/* 開關選項 */
/* 如果定義了該選項，使用不帶緩沖的readline函數 */
#cmakedefine USE_READLINE_NOBUFFER

/* 如果存在IPv4則定義 */
#cmakedefine IPV4 1

/* 如果存在IPv6則定義 */
#cmakedefine IPV6 1

#cmakedefine HAVE_WCHAR_T
#cmakedefine HAVE_LONG_LONG
#cmakedefine HAVE_LONG_DOUBLE
#cmakedefine HAVE_LONG_LONG_DOUBLE

#cmakedefine CHAR_SIZE ${CHAR_SIZE}
#cmakedefine WCHAR_T_SIZE ${WCHAR_T_SIZE}
#cmakedefine SHORT_SIZE ${SHORT_SIZE}
#cmakedefine INT_SIZE ${INT_SIZE}
#cmakedefine LONG_SIZE ${LONG_SIZE}
#cmakedefine LONG_LONG_SIZE ${LONG_LONG_SIZE}
#cmakedefine FLOAT_SIZE ${FLOAT_SIZE}
#cmakedefine DOUBLE_SIZE ${DOUBLE_SIZE}
#cmakedefine LONG_DOUBLE_SIZE ${LONG_DOUBLE_SIZE}
#cmakedefine LONG_LONG_DOUBLE_SIZE ${LONG_LONG_DOUBLE_SIZE}
#cmakedefine POINTER_SIZE ${POINTER_SIZE}
